<div
        id="brander-advanced-map-{{ serial }}"
        class="brander-map"
        {#data-department="{{ departmentData }}"#}
>
    <advanced-map
            map-id="{{ serial }}"
            :options="defaultMapOptions"
            json-departments="{{ departmentData }}"
            json-labels="{{ labels }}"
            json-filters="{{ filters }}"
            json-filters-data="{{ filtersData }}"
            json-lang="{{ lang }}"
    ></advanced-map>
</div>
